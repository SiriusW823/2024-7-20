# c
'''
#include <stdio.h>

int main() {
    int i, j;
    
    // 生成9x9乘法表
    for (i = 1; i <= 9; i++) {
        for (j = 1; j <= 9; j++) {
            printf("%d * %d = %2d ", i, j, i * j);
        }
        printf("\n");
    }
    
    return 0;
}
'''
# c++
'''
#include <iostream>
using namespace std;

int main() {
    // 生成9*9乘法表
    for (int i = 1; i <= 9; ++i) {
        for (int j = 1; j <= 9; ++j) {
            cout << i << " * " << j << " = " << i * j << "\t";
        }
        cout << endl;
    }
    return 0;
}

'''
